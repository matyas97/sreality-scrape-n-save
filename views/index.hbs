    crossorigin="anonymous"></script>
</head>

<body style="padding: 5rem 2rem 5rem 2rem;">
  <div class="container">
    <div style="display: grid; grid-gap: 2rem; grid-template-columns: repeat(auto-fit, minmax(10rem, 25rem));">
      {{#each properties}}
      <a href="{{ this.url }}" target="_blank" class="col">
        <div class="card" style="width: 25rem;">
          <img src="{{ this.thumbnailUrl }}" class="card-img-top" alt="...">
          <div class="card-body">
            <h5 class="card-title" style="text-align: center;">{{ this.title }}</h5>
          </div>
        </div>
      </a>
      {{/each}}
    </div>
